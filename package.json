{
  "name": "tina-docker",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "dev": "TINA_PUBLIC_IS_LOCAL=true concurrently \"tinacms dev\" \"tsc --watch\" \"nodemon dist/app.js\" \"nodemon --watch site eleventy --input='site'\"",
    "dev:prod": "TINA_PUBLIC_IS_LOCAL=false concurrently \"tinacms dev\" \"tsc --watch\" \"nodemon dist/app.js\" \"nodemon --watch site eleventy --input='site'\"",
    "build": "tinacms build --partial-reindex --verbose && eleventy --input='site' && tsc",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0"
  },
  "dependencies": {
    "@tinacms/cli": "^1.5.34",
    "@tinacms/datalayer": "^1.2.29",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "mongodb-level": "^0.0.3",
    "next": "^13.4.19",
    "next-auth": "^4.22.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "serverless-http": "^3.2.0",
    "tinacms": "^1.5.24",
    "tinacms-authjs": "^1.0.1",
    "tinacms-gitprovider-github": "^1.0.0",
    "ts-node": "^10.9.1",
    "tsx": "^3.12.7",
    "wrangler": "^3.4.0"
  },
  "devDependencies": {
    "@11ty/eleventy": "^2.0.1",
    "@types/cookie-parser": "^1.4.6",
    "@types/cors": "^2.8.16",
    "@types/express": "^4.17.20",
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vercel/node": "^3.0.7",
    "concurrently": "^8.2.2",
    "eslint": "^8.45.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.3",
    "nodemon": "^3.0.1",
    "typescript": "^5.0.2"
  }
}
